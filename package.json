{
  "name": "data-upload-validation-system",
  "version": "1.0.0",
  "description": "A robust system for managing user authentication and CSV data uploads.",
  "main": "build/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node build/index.js",
    "test": "NODE_ENV=test vitest",
    "db:migrate": "ts-node src/db/scripts/dbMigrate.ts",
    "db:migrate:down": "ts-node src/db/scripts/dbMigrate.ts down",
    "db:create": "ts-node src/db/scripts/dbCreate.ts",
    "db:drop": "ts-node src/db/scripts/dbDrop.ts",
    "db:delete": "ts-node src/db/scripts/dbDelete.ts",
    "db:reset": "npm run db:drop && npm run db:create && npm run db:migrate",
    "db:seed": "ts-node src/db/scripts/dbSeed.ts",
    "db:truncate": "ts-node src/db/utils.ts truncate"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.5",
    "umzug": "^3.8.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.12",
    "@types/pg": "^8.11.6",
    "@types/qs": "^6.9.15",
    "@types/sinon": "^17.0.3",
    "@types/supertest": "^6.0.2",
    "nodemon": "^3.1.1",
    "sinon": "^17.0.1",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
